#include "menu_edit.h"
#include "menu_struct.h"

void menu_change_value(menu_id_t id, int8_t delta) {
    switch(id) {
{% for category in unique_categories if category != 'callback' %}
        case MENU_CATEGORY_{{category.upper()}}:
            change_{{category}}_value(id, delta);
            break;
{% endfor %}
        default:
            break;
    }
}

void menu_next_value(menu_id_t id) {
    switch(id) {
{% for category in unique_categories if category != 'callback' %}
        case MENU_CATEGORY_{{category.upper()}}:
            next_{{category}}_value(id);
            break;
{% endfor %}
        default:
            break;
    }
}

